<%= simple_form_for :query, url: venues_path, method: "GET" do |f| %>
  <%= f.input :venue_type, label: "Restaurant Cusine",  collection: $venue_types, as: :check_boxes %>
  <%= f.input :venue_attribute, label: "Restaurant Attributes", collection: $venue_attributes, as: :check_boxes %>
  <%= f.submit "Search", class: "btn btn-primary" %>
  <%= link_to "Reset", venues_path  %>
<% end %>
