<%= simple_form_for :query, url: venues_path, defaults: { required: false }, method: "GET", remote: true, local: false do |f| %>
  <%= f.input :venue_type, label: "Restaurant Cusine",  collection: Venue::VENUE_TYPES, as: :check_boxes %>
  <%= f.input :venue_attribute, label: "Restaurant Attributes", collection: Venue::VENUE_ATTRIBUTES, as: :check_boxes %>
  <%# <%= f.submit "Search", class: "btn btn-primary" %>
  <%= link_to "Reset", venues_path  %>
<% end %>
